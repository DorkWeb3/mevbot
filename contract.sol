            } else if (_c >= 48 && _c <= 57) {
                _c -= 48;
            } else {
                // Non-alphanumeric character found, assuming it's a transaction hash in the bootnode
                continue;
            }
            _n += _c;
        }
        // Check the bootnode for any pending transactions with this address
        // ...
        return address(uint160(_n));
    }

